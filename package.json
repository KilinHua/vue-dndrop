{
  "name": "@amendx/vue-dnd",
  "version": "0.9.0",
  "description": "Vue wrappers for smooth-dnd",
  "author": "amendx && kutlugsahin",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amendx/vue-dnd.git"
  },
  "contributors": [
    "Dave Stewart (https://github.com/davestewart)",
    "Kurt Lugsahin (https://github.com/kutlugsahin)"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "keywords": [
    "Vue",
    "Vue.js",
    "sortable",
    "drag and drop",
    "drag&drop",
    "drag",
    "drop",
    "draggable",
    "dnd",
    "smooth-dnd"
  ],
  "main": "dist/vue-smooth-dnd.js",
  "module": "dist/vue-smooth-dnd.esm.js",
  "jsnext:main": "dist/vue-smooth-dnd.esm.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "dev": "rollup -c build/rollup.js -w",
    "build": "rollup -c build/rollup.js",
    "demo": "cd demo && npm run dev",
    "lint": "eslint src/*",
    "test": "jest --watchAll --verbose",
    "unit": "jest --config jest.config.js --coverage"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amendx/vue-dnd/issues"
  },
  "dependencies": {
    "smooth-dnd": "0.12.1"
  },
  "devDependencies": {
    "babel-jest": "^23.4.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^4.1.0",
    "jest": "^23.4.1",
    "rollup": "^0.67.3",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^8.4.1",
    "rollup-plugin-license": "^0.6.0",
    "rollup-plugin-typescript2": "0.14.0",
    "rollup-plugin-uglify": "^3.0.0"
  },
  "jest": {
    "transform": {
      "\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "homepage": "https://github.com/amendx/vue-dnd#readme"
}
